{
  "template": [
    "l1p_index*"
  ],
  "mappings": {
    "l1p_log": {
      "_all": {
        "norms": false
      },
      "dynamic_templates": [
        {
          "strings_as_keyword": {
            "match_mapping_type": "string",
            "mapping": {
              "ignore_above": 1024,
              "type": "keyword"
            }
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "l1p_trace_id": {
          "type": "keyword"
        },
        "l1p_service_id": {
          "type": "keyword"
        },
        "l1p_environment": {
          "type": "keyword"
        },
        "beat": {
          "properties": {
            "hostname": {
              "type": "keyword",
              "ignore_above": 1024
            },
            "name": {
              "type": "keyword",
              "ignore_above": 1024
            },
            "version": {
              "type": "keyword",
              "ignore_above": 1024
            },
            "processing_timestamp": {
              "type": "date"
            }
          }
        },
        "input_type": {
          "type": "keyword",
          "ignore_above": 1024
        },
        "message": {
          "type": "text",
          "norms": false
        },
        "meta": {
          "properties": {
            "cloud": {
              "properties": {
                "availability_zone": {
                  "type": "keyword",
                  "ignore_above": 1024
                },
                "instance_id": {
                  "type": "keyword",
                  "ignore_above": 1024
                },
                "machine_type": {
                  "type": "keyword",
                  "ignore_above": 1024
                },
                "project_id": {
                  "type": "keyword",
                  "ignore_above": 1024
                },
                "provider": {
                  "type": "keyword",
                  "ignore_above": 1024
                },
                "region": {
                  "type": "keyword",
                  "ignore_above": 1024
                }
              }
            }
          }
        },
        "offset": {
          "type": "long"
        },
        "source": {
          "type": "keyword",
          "ignore_above": 1024
        },
        "tags": {
          "type": "keyword",
          "ignore_above": 1024
        },
        "type": {
          "type": "keyword",
          "ignore_above": 1024
        }
      }
    }
  }
}